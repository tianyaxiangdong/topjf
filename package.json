{
  "name": "topjf",
  "version": "3.197.10",
  "description": "TOPJF.笔记",
  "license": "MIT",
  "scripts": {
    "build:vite": "vuepress-vite build docs",
    "dev:vite": "vuepress-vite dev docs",
    "dev:vite-clean": "vuepress-vite dev docs --clean-cache",
    "test": "echo demo test out!",
    "commit": "git-cz",
    "lint-md": "markdownlint **/*.md --ignore node_modules",
    "linux-lint-md": "markdownlint '**/*.md' --ignore node_modules",
    "prepare": "husky install",
    "update-package": "pnpm dlx vp-update"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.61",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-search-pro": "2.0.0-beta.197",
    "vuepress-theme-hope": "2.0.0-beta.197",
    "vuepress-vite": "2.0.0-beta.61"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "markdownlint-cli": "^0.33.0"
  },
  "packageManager": "pnpm@7.30.0",
  "engines": {
    "node": ">=14",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "webpack",
        "vuepress",
        "@commitlint/cli",
        "sass-loader"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
